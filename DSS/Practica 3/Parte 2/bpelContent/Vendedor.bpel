                            </bpel:to>
                        </bpel:copy>
                        
                    </bpel:assign>
                    
                    <bpel:if name="If">
                        <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$entrada = "ordenador"]]></bpel:condition>
                        <bpel:assign validate="no" name="AsignarPrecio">
                            <bpel:copy>
                                <bpel:from>
                                    <bpel:literal xml:space="preserve">9950.0</bpel:literal>
                                </bpel:from>
                                <bpel:to part="parameters" variable="clientResponse">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:out]]></bpel:query>
                                </bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        <bpel:else>
                            <bpel:assign validate="no" name="AsignarCero">
                                <bpel:copy>
                                    <bpel:from>
                                        <bpel:literal xml:space="preserve">0.0</bpel:literal>
                                    </bpel:from>
                                    <bpel:to part="parameters" variable="clientResponse">
                                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:out]]></bpel:query>
                                    </bpel:to>
                                </bpel:copy>
                            </bpel:assign>
                        </bpel:else>
                    </bpel:if>
                    <bpel:reply name="ReplyPrecio" partnerLink="client" operation="existeProducto" portType="tns:Vendedor" variable="clientResponse"></bpel:reply>
                </bpel:sequence>
            </bpel:onMessage>
            <bpel:onMessage partnerLink="client" operation="recibirOferta" portType="tns:Vendedor" variable="clientRequest1">
                <bpel:sequence>
                    <bpel:assign validate="no" name="AsignarOferta">
                        <bpel:copy>
                            <bpel:from><bpel:literal><tns:recibirOfertaResponse xmlns:tns="http://vendedor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:out>true</tns:out>
</tns:recibirOfertaResponse>
</bpel:literal></bpel:from>
                            <bpel:to variable="clientResponse1" part="parameters"></bpel:to>
                        </bpel:copy>
                        <bpel:copy>
                            <bpel:from part="parameters" variable="clientRequest1">
                                <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:in]]></bpel:query>
                            </bpel:from>
                            <bpel:to variable="oferta">
                            </bpel:to>
                        </bpel:copy>
                    </bpel:assign>
                    <bpel:if name="If1">
                        <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$oferta < 9000.0]]></bpel:condition>
                        <bpel:assign validate="no" name="AsignarRechazar">
                            <bpel:copy>
                                <bpel:from>
                                    <bpel:literal xml:space="preserve">false</bpel:literal>
                                </bpel:from>
                                <bpel:to part="parameters" variable="clientResponse1">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:out]]></bpel:query>
                                </bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        <bpel:else>
                            <bpel:assign validate="no" name="AsignarAceptar">
                                <bpel:copy>
                                    <bpel:from>
                                        <bpel:literal xml:space="preserve">true</bpel:literal>
                                    </bpel:from>
                                    <bpel:to part="parameters" variable="clientResponse1">
                                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:out]]></bpel:query>
                                    </bpel:to>
                                </bpel:copy>
                            </bpel:assign>
                        </bpel:else>
                    </bpel:if>
                    <bpel:reply name="ReplyOferta" partnerLink="client" operation="recibirOferta" portType="tns:Vendedor" variable="clientResponse1"></bpel:reply>
