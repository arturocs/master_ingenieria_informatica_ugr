                </bpel:sequence>
            </bpel:onMessage>
            <bpel:onMessage partnerLink="client" operation="existeProducto" portType="tns:Vendedor" variable="clientRequest">
                <bpel:sequence>
                    <bpel:assign validate="no" name="AsignaEntrada">
                        
                        
                        <bpel:copy>
                            <bpel:from><bpel:literal><tns:existeProductoResponse xmlns:tns="http://vendedor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:out>0.0</tns:out>
</tns:existeProductoResponse>
</bpel:literal></bpel:from>
