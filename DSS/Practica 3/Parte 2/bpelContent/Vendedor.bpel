        <bpel:variable name="clientResponse1" messageType="tns:recibirOfertaResponse"></bpel:variable>
        
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in Vendedor.wsdl 
             -->
        
        <bpel:pick name="ElegirAccion" createInstance="yes">
            
            <bpel:onMessage partnerLink="client" operation="process" portType="tns:Vendedor" variable="input">
                <bpel:sequence>
                    <bpel:assign validate="no" name="AsignaDefault">
                        <bpel:copy>
